{
  "name": "oicpp-sample-tester",
  "version": "1.2.3-alpha2",
  "description": "从 OJ 平台获取题目样例并发送到 OICPP 的油猴脚本",
  "main": "dist/fetch_tampermonkey.user.js",
  "userscript": {
    "name": "OICPP sampleTester",
    "namespace": "https://oicpp.mywwzh.top/",
    "description": "从 OJ 平台获取题目样例并发送到 OICPP 的油猴脚本",
    "author": "Mr_Onion & mywwzh",
    "match": [
      "https://www.luogu.com.cn/*",
      "https://htoj.com.cn/cpp/oj/problem/detail?pid=*",
      "https://atcoder.jp/contests/*/tasks/*",
      "https://codeforces.com/contest/*/problem/*",
      "https://codeforces.com/problemset/problem/*/*",
      "https://codeforces.com/gym/*/problem/*",
      "https://hydro.ac/*",
      "https://www.yanhaozhe.cn/*"
    ],
    "grant": "GM_xmlhttpRequest",
    "connect": [
      "http://127.0.0.1:20030",
      "127.0.0.1"
    ]
  },
  "scripts": {
    "build": "tsc && node scripts/bundle.js && node scripts/generate-userscript.js",
    "dev": "tsc --watch"
  },
  "keywords": [],
  "author": "Mr_Onion & mywwzh",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "esbuild": "^0.20.0"
  }
}
