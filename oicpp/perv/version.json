{
  "name": "oicpp-sample-tester",
  "version": "1.2.4-alpha4",
  "description": "从 OJ 平台获取题目样例并发送到 OICPP 的油猴脚本",
  "main": "dist/fetch_tampermonkey.user.js",
  "userscript": {
    "name": "OICPP sampleTester",
    "namespace": "https://oicpp.mywwzh.top/",
    "description": "从 OJ 平台获取题目样例并发送到 OICPP 的油猴脚本",
    "author": "Mr_Onion & mywwzh",
    "match": [
      "*://*/*"
    ],
    "grant":[
      "GM_xmlhttpRequest",
      "GM_info",
      "GM_openInTab",
      "GM_setValue",
      "GM_getValue"
    ],
    "connect": [
      "http://127.0.0.1:20030",
      "127.0.0.1",
      "onion-static.netlify.app"
    ]
  },
  "scripts": {
    "build": "tsc && node scripts/bundle.js && node scripts/generate-userscript.js",
    "dev": "tsc --watch"
  },
  "keywords": [],
  "author": "Mr_Onion & mywwzh",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.0.0",
    "esbuild": "^0.20.0"
  }
}
